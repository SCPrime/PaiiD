    "NEXT_PUBLIC_APP_NAME": "PaiiD",
    "BACKEND_API_BASE_URL": "https://paiid-backend.onrender.com",
    "NEXT_PUBLIC_BACKEND_API_BASE_URL": "https://paiid-backend.onrender.com"
