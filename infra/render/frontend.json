{
  "name": "paiid-frontend",
  "runtime": "node",
  "plan": "free",
  "region": "oregon",
  "branch": "main",
  "rootDir": "frontend",
  "buildCommand": "npm install && npm run build",
  "startCommand": "node server.js",
  "env": {
    "NEXT_PUBLIC_API_TOKEN": "secret",
    "NEXT_PUBLIC_BACKEND_API_BASE_URL": "https://paiid-backend.onrender.com",
    "NEXT_PUBLIC_ANTHROPIC_API_KEY": "secret",
    "NODE_ENV": "production",
    "PORT": "3000"
  }
}
